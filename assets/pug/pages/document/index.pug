//- sidebar
-var sidebar = true;

//- feather
-var feather= true;

//- custom select
-var customSelect= true;

//- customizer 
-var customizer=true;

//- Breadcrumbs 
-var pageTitle="Govo";
-var breadcrumb=['Getting Started']; 

//- all JSON  
include ../../data/all-data

//- all mixin
include ../../mixin/mixin

doctype html
html(lang='en')
    include ../../layout/header-files
    body(dir="ltr" class="unique-skin")
        main.page-wrapper
            div.page-body-wrapper(class="xl:w-full 4xl:w-[calc(100%_-_14.375rem)] w-[calc(100%_-_15.8125rem)] ml-auto transition-all duration-300")
                include ../../layout/header/header     
                // Sidebar Start 
                div.page-sidebar.sidebar-dark.unique-sidebar
                    include ../../layout/unique
                // Sidebar End 

                // Page Body Start
                div.page-body
                    div.grid.grid-cols-12.gap-card-gap 
                        div.card.col-span-12        
                            div.card-header.pb-2.header-bottom-border 
                                h3.text-lg.font-bold Let's begin
                                p Thank you for choosing the Govo tailwind admin template
                            div.card-body
                                p(class="text-sm")  "We are pleased that you have chosen Govo admin theme for your site and we will not disappoint you with your choice. Govo is an excellent theme develop with a concise approach. The code written is a clean and well structured."
                                p(class="mt-3 text-sm text-primary")  Govo Admin is a responsive HTML template that is based on the CSS, Pure Javascript, 
                                    mark  &nbsp;framework tailwind 
                                    | &nbsp;and it is built with postcss, Gulp and Pug. postcss compiler makes it easier to code and customize. If you are unfamiliar with tailwind or Gulp, visit their website and read through the documentation. All of components have been modified to fit the style of Govo Admin and provide a consistent look throughout the template.
                                p(class="mt-3 text-sm")  Before you start working with the template, we suggest you go through the pages that are bundled with the theme. Most of the template example pages contain quick tips on how to create or use a component which can be really helpful when you need to create something on the fly.

                        div.card.col-span-12        
                            div.card-header.pb-2.header-bottom-border 
                                h3.text-lg.font-bold GOVO Dependencies
                            div.card-body
                                ul
                                    li(class="text-sm leading-7") "gulp-autoprefixer": 
                                        span(class="text-sm font-bold text-primary") "^8.0.0" 
                                    li(class="text-sm leading-7") "gulp-livereload": 
                                        span(class="text-sm font-bold text-primary") "^4.0.2" 
                                    li(class="text-sm leading-7") "gulp-plumber": 
                                        span(class="text-sm font-bold text-primary") "^1.2.1" 
                                    li(class="text-sm leading-7") "gulp-pug": 
                                        span(class="text-sm font-bold text-primary") "^5.0.0" 
                                    li(class="text-sm leading-7") "gulp-purgecss": 
                                        span(class="text-sm font-bold text-primary") "^4.1.3" 
                                    li(class="text-sm leading-7") "gulp-watch": 
                                        span(class="text-sm font-bold text-primary") "^5.0.1" 
                                    li(class="text-sm leading-7") "save": 
                                        span(class="text-sm font-bold text-primary") "^2.5.0" 
                                    li(class="text-sm leading-7") "tailwindcss-animatecss": 
                                        span(class="text-sm font-bold text-primary") "^1.0.7" 

                // Page Body End 
                +footer("sm:flex-col flex sm:gap-1 gap-2 items-center justify-between py-[1.1875rem] shadow-footer bg-white px-body-space mt-card-gap")
            include ../../components/customizer

    include ../../layout/footer-files